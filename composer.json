{
    "name": "axephp/blade",
    "description": "AxE PHP Framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin Panta",
            "email": "hacktivistic@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "src/AxE/Framework/supporter.php",
            "src/Support/supporter.php"
        ],
        "psr-4": { "Blade\\": "src/" }
    },
    "minimum-stability": "dev",

    "require": {
        "php": ">=5.6",
        "symfony/http-foundation": "^3.2",
        "illuminate/database": "^5.2"
    }, 
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
